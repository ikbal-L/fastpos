    <SolidColorBrush x:Uid="SolidColorBrush_9" x:Key="Button.Disabled.Foreground" Color="#FF838383"/>
    <Style x:Uid="Style_17" x:Key="ButtonStyle1" TargetType="{x:Type Button}">
        <Setter x:Uid="Setter_112" Property="FocusVisualStyle" Value="{StaticResource FocusVisual}"/>
        <Setter x:Uid="Setter_113" Property="Background" Value="{StaticResource Button.Static.Background}"/>
        <Setter x:Uid="Setter_114" Property="BorderBrush" Value="{StaticResource Button.Static.Border}"/>
        <Setter x:Uid="Setter_115" Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
        <Setter x:Uid="Setter_116" Property="BorderThickness" Value="1"/>
        <Setter x:Uid="Setter_117" Property="HorizontalContentAlignment" Value="Center"/>
        <Setter x:Uid="Setter_118" Property="VerticalContentAlignment" Value="Center"/>
        <Setter x:Uid="Setter_119" Property="Padding" Value="1"/>
        <Setter x:Uid="Setter_120" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_6" TargetType="{x:Type Button}">
                    <Border x:Uid="Border_4" x:Name="border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0" Background="{TemplateBinding Background}" SnapsToDevicePixels="true">
                        <ContentPresenter x:Uid="contentPresenter" x:Name="contentPresenter" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_17" Property="IsDefaulted" Value="true">
                            <Setter x:Uid="Setter_121" Property="BorderBrush" TargetName="border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                        </Trigger>
                        <Trigger x:Uid="Trigger_18" Property="IsMouseOver" Value="true">
                            <Setter x:Uid="Setter_122" Property="Background" TargetName="border" Value="{StaticResource Button.MouseOver.Background}"/>
                            <Setter x:Uid="Setter_123" Property="BorderBrush" TargetName="border" Value="{StaticResource Button.MouseOver.Border}"/>
                        </Trigger>
                        <Trigger x:Uid="Trigger_19" Property="IsPressed" Value="true">
                            <Setter x:Uid="Setter_124" Property="Background" TargetName="border" Value="{StaticResource Button.Pressed.Background}"/>
                            <Setter x:Uid="Setter_125" Property="BorderBrush" TargetName="border" Value="{StaticResource Button.Pressed.Border}"/>
                        </Trigger>
                        <Trigger x:Uid="Trigger_20" Property="IsEnabled" Value="false">
                            <Setter x:Uid="Setter_126" Property="Background" TargetName="border" Value="{StaticResource Button.Disabled.Background}"/>
                            <Setter x:Uid="Setter_127" Property="BorderBrush" TargetName="border" Value="{StaticResource Button.Disabled.Border}"/>
                            <Setter x:Uid="Setter_128" Property="TextElement.Foreground" TargetName="contentPresenter" Value="{StaticResource Button.Disabled.Foreground}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

